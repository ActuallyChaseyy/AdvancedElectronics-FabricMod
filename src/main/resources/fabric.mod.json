{
  "schemaVersion": 1,
  "id": "advancedelectronics",
  "version": "${version}",

  "name": "Advanced Electronics",
  "description": "A fabric mod that brings in more electronic components to minecraft!",
  "authors": [
    "Chaseyy"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.chaseyy.advancedelectronics.AdvancedElectronics"
    ]
  },
  "mixins": [
    "advancedelectronics.mixins.json"
